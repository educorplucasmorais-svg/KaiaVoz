# MySQL connection string (Hostinger)
# Format: mysql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL="mysql://user:password@host:3306/kaia"

PORT=3060

# Allow specific origin in production (e.g., your Vercel domain). Leave unset for dev.
CORS_ORIGIN=

# Enable GPT-5.2 Preview for all clients
# Set to 'true' or 'false'. If unset, defaults to enabled in non-production.
GPT52_PREVIEW=

# ElevenLabs TTS Configuration (optional)
# If set, the server will use ElevenLabs for text-to-speech instead of Edge TTS.
# Get your API key from https://elevenlabs.io/
ELEVENLABS_API_KEY=

# ElevenLabs Voice ID (optional, defaults to 'pNInz6obpgDQGcFmaJgB' - Adam)
# Find voice IDs at https://elevenlabs.io/app/voice-library
ELEVENLABS_VOICE_ID=

# ElevenLabs Model ID (optional, defaults to 'eleven_multilingual_v2')
# Options: eleven_multilingual_v2, eleven_monolingual_v1, eleven_turbo_v2
ELEVENLABS_MODEL_ID=
